; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Control",
"class": "Board",
"language": "GDScript",
"path": "res://components/Board.gd"
}, {
"base": "MarginContainer",
"class": "LayeredContainer",
"language": "GDScript",
"path": "res://ui/LayeredContainer.gd"
}, {
"base": "TextureButton",
"class": "Piece",
"language": "GDScript",
"path": "res://components/Piece.gd"
}, {
"base": "Container",
"class": "Players",
"language": "GDScript",
"path": "res://ui/Players.gd"
}, {
"base": "TextureButton",
"class": "SimpleButton",
"language": "GDScript",
"path": "res://ui/SimpleButton.gd"
} ]
_global_script_class_icons={
"Board": "",
"LayeredContainer": "",
"Piece": "",
"Players": "",
"SimpleButton": ""
}

[application]

config/name="Nim"
config/description="A mathematical game of strategy."
run/main_scene="res://Game.tscn"
config/icon="res://assets/icon.png"

[audio]

default_bus_layout="res://resource/default_bus_layout.tres"

[autoload]

Globals="*res://Globals.gd"
Audio="*res://Audio.tscn"

[display]

window/size/width=600
window/size/height=800
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="expand"

[importer_defaults]

ogg_vorbis={
"loop": false,
"loop_offset": 0
}
mp3={
"loop": false,
"loop_offset": 0
}

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
quality/intended_usage/framebuffer_allocation=0
quality/intended_usage/framebuffer_allocation.mobile=1
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_clear_color=Color( 0.941176, 0.913725, 0.823529, 1 )
